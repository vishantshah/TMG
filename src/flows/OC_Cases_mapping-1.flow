      )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_8045863205</name>
        <dataType>String</dataType>
        <expression>IF({!myVariable_current.Subject} = &quot;Outbound Renewal tracker&quot;, &quot;Renewal&quot;,
     IF({!myVariable_current.Subject} = &quot;DD Winback&quot;, &quot;DD Winback&quot;,
          IF({!myVariable_current.Subject} = &quot;DDCX&quot;, &quot;DDCX&quot;,{!myVariable_current.Subject}
               )
      )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
ISCHANGED([Case].Subject ), 
ISCHANGED([Case].OC_Call_Stage__c ), 
ISCHANGED([Case].OC_Outcome__c ), 
ISCHANGED([Case].OC_No_Sale_Reason__c)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR(
ISCHANGED({!myVariable_current.Subject} ), 
ISCHANGED({!myVariable_current.OC_Call_Stage__c} ), 
ISCHANGED({!myVariable_current.OC_Outcome__c} ), 
ISCHANGED({!myVariable_current.OC_No_Sale_Reason__c})
)</expression>
    </formulas>
