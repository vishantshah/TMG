        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF([Case].Subject = &quot;Outbound Renewal tracker&quot;, &quot;Renewal&quot;,
     IF([Case].Subject = &quot;DD Winback&quot;, &quot;DD Winback&quot;,
          IF([Case].Subject = &quot;DDCX&quot;, &quot;DDCX&quot;,[Case].Subject
