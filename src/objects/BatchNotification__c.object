<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This flag is used to identify the status of a particular batch whether it is running or not. The value in this setting corresponds to a particular batch. If the value exists, then the batch is running, if not then the batch is not running.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <label>Batch Notification</label>
</CustomObject>
