    <webLinks>
        <fullName>Manage_Refunds</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Manage Refunds</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 

try{
    //This functionality is available only for tmg super user and system administrator
    var ProfileName=&apos;{!$Profile.Name}&apos;;

    if(ProfileName==&apos;TMG Super User&apos; || ProfileName==&apos;System Administrator&apos; || ProfileName==&apos;TMG Subs Administrator&apos; || ProfileName==&apos;System Administrator (Custom)&apos;){

         //get the current account id
         var accountId=&apos;{!Account.Id}&apos;;
    
         //prepare the soql to get the active billing account for this account, only one active account 
         var query = &quot;SELECT Id,Name,Zuora__Account__c from Zuora__CustomerAccount__c where Zuora__Status__c =&apos;Active&apos; and Zuora__Account__c=&apos;&quot; + accountId+ &quot;&apos; LIMIT 1&quot;; 

         //run the soql
         var records = sforce.connection.query(query); 
         var recordsArray = records.getArray(&apos;records&apos;); 

         //check if there an active billing account or no and act accordingly
         if(recordsArray.length &gt; 0){
            window.open(&apos;/apex/Manage_Billing_Account?id=&apos;+recordsArray[0].Id, &apos;_self&apos;);
         } else {
            alert(&apos;{!$Label.No_Billing_Account_Found}&apos;); 
         }
    }else {
       alert(&apos;{!$Label.Manage_Billing_Account_Insufficient_Privileges}&apos;);
    }
}catch(e){ 
    alert(&apos;{!$Label.Manage_Billing_Account_Exception}&apos; +e); 
}</url>
