    <validationRules>
        <fullName>Account_First_and_Last_Name_Length</fullName>
        <active>true</active>
        <description>Last Name should be more than 1 character</description>
        <errorConditionFormula>AND(
    NOT($Profile.Name = &quot;Zuora 360 Sync&quot;), 
    RecordType.Name ==&apos;Registrant&apos;,
    (AND(LEN(LastName)&lt;2,NOT(ISBLANK(LastName))))
   )</errorConditionFormula>
        <errorMessage>Please enter the valid last name. Last Name should be more than one character</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Age_will_be_greater_than_16</fullName>
        <active>true</active>
        <description>PP-754 changes on age validation</description>
        <errorConditionFormula>AND(NOT($Profile.Name = &quot;Zuora 360 Sync&quot;), ISNEW() ,OR((ABS(TODAY()- PersonBirthdate ) &lt; 5844),(ABS(TODAY()-PersonBirthdate ) &gt; 36525)))</errorConditionFormula>
        <errorDisplayField>X3rd_Party_Email_Offers__pc</errorDisplayField>
        <errorMessage>Age Should be greater than 16 years and less than 100 years</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Checked_Closed_Date_Required</fullName>
        <active>true</active>
        <description>Ensures that a Closed Date is entered if the Closed checkbox is checked.</description>
        <errorConditionFormula>IF( 
   OR( 
      $Profile.Name = &quot;TMG System&quot;,
      $Profile.Name = &quot;Zuora 360 Sync&quot;
      ),
  false,
  AND(
      Closed__c = True, 
      ISNULL(Closed_Date__c), 
      IsPersonAccount = False
     )
)</errorConditionFormula>
        <errorDisplayField>Closed_Date__c</errorDisplayField>
        <errorMessage>Enter a Closed Date if the Closed checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_bith_validation</fullName>
        <active>false</active>
        <description>PP-754, Age Should be greater than 16 years and less than 100 years</description>
        <errorConditionFormula>AND( OR(ISNEW(), DATEVALUE(CreatedDate) &gt; DATE(2012,11,23)) ,OR((ABS(TODAY()-DateofBirth__c) &lt; 5844),(ABS(TODAY()-DateofBirth__c) &gt; 36525)))</errorConditionFormula>
        <errorDisplayField>X3rd_Party_Email_Offers__pc</errorDisplayField>
        <errorMessage>Age Should be greater than 16 years and less than 100 years</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Executor_fields_read_only</fullName>
        <active>true</active>
        <description>This rule ensures executor fields can only be edited if the Subscriber is marked as deceased</description>
        <errorConditionFormula>IF( OR($Profile.Name = &quot;TMG System&quot;,$Profile.Name = &quot;Zuora 360 Sync&quot;),false,
AND(OR( ISCHANGED( PersonOtherCity ), ISCHANGED( PersonOtherCountry ), ISCHANGED(PersonOtherState),ISCHANGED( PersonOtherStreet),ISCHANGED( PersonOtherPostalCode)), Deceased__pc &lt;&gt; true)
)</errorConditionFormula>
        <errorDisplayField>Deceased__pc</errorDisplayField>
        <errorMessage>An executor address can not be entered unless the Subscriber is marked as Deceased</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fax_Start_Zero</fullName>
        <active>true</active>
        <description>Validates the Fax field to ensure it starts with zero.</description>
        <errorConditionFormula>IF(OR($Profile.Name = &quot;TMG System&quot;, $Profile.Name = &quot;System Administrator&quot;, $Profile.Name = &quot;Zuora 360 Sync&quot;, $Profile.Name = &quot;System Administrator (Custom)&quot;),false,
AND(NOT(BEGINS( Fax,&quot;0&quot;)), OR(PersonMailingCountry=&apos;GB&apos;,PersonMailingCountry=&apos;UK&apos;))
)</errorConditionFormula>
        <errorDisplayField>Fax</errorDisplayField>
        <errorMessage>Fax number must start with zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Home_Phone_Start_Zero</fullName>
        <active>true</active>
        <description>Validates the Phone field to ensure it starts with zero.</description>
        <errorConditionFormula>IF( 
 OR($Profile.Name = &quot;TMG System&quot;,
    $Profile.Name = &quot;System Administrator&quot;, 
    $Profile.Name = &quot;Zuora 360 Sync&quot;, 
    $Profile.Name = &quot;System Administrator (Custom)&quot;,
    $Profile.Name = &quot;Clock&quot;
  )
  ,false,
  AND(NOT(BEGINS(PersonHomePhone ,&quot;0&quot;)), 
     OR(PersonMailingCountry=&apos;GB&apos;,PersonMailingCountry=&apos;UK&apos;)
  )
)</errorConditionFormula>
        <errorDisplayField>PersonHomePhone</errorDisplayField>
        <errorMessage>Phone number must start with zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_Name_Only_Text</fullName>
        <active>true</active>
        <description>Last Name can only contain letters</description>
        <errorConditionFormula>/* Does not impact Retailer */
AND(
OR( $RecordType.Name  == &quot;Individual Subscriber&quot;,
$RecordType.Name == &quot;Digital Subscriber&quot;,
$RecordType.Name  == &quot;Registrant&quot;
),
NOT($Profile.Name = &apos;Web API&apos;),
NOT($Profile.Name = &quot;Zuora 360 Sync&quot;), 
NOT(REGEX(LastName, &apos;^[^0-9!\&quot;#&amp;$%&amp;()*+,/:;&lt;=&gt;?@\\[\\]\\^\\{|}_~]+$&apos;))
)</errorConditionFormula>
        <errorMessage>Last Name can only contain letters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Loyalty_Portal_Email_Change</fullName>
        <active>false</active>
        <description>Subscribers whose account has been linked to a Loyalty Portal user can only change their email addresses through the portal.</description>
        <errorConditionFormula>IF( $Profile.Name = &quot;TMG System&quot; || $Profile.Name = &quot;FirstClarityApi&quot; || $Profile.Name = &quot;TMG Third Party API&quot;
  ,false
  ,IF (NOT(ISNULL(Guid__c) || 
      ISBLANK(Guid__c) ||
      PRIORVALUE(Guid__c) == null ||
      PRIORVALUE(Guid__c) == &apos;&apos; ||
      PRIORVALUE(PersonEmail) == null ) &amp;&amp;  
      PersonEmail &lt;&gt;  PRIORVALUE(PersonEmail)
    , true
    , false)
)</errorConditionFormula>
        <errorMessage>This subscriber is registered on the loyalty portal. Email address updates must take place via the portal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mobile_Start_Zero</fullName>
        <active>true</active>
        <description>Validates the Mobile field to ensure it starts with zero.</description>
        <errorConditionFormula>IF( OR($Profile.Name = &quot;TMG System&quot;,$Profile.Name = &quot;System Administrator&quot;, $Profile.Name = &quot;Zuora 360 Sync&quot;, $Profile.Name = &quot;System Administrator (Custom)&quot;),false,
AND(NOT(BEGINS( PersonMobilePhone,&quot;0&quot;)), OR(PersonMailingCountry=&apos;GB&apos;,PersonMailingCountry=&apos;UK&apos;))
)</errorConditionFormula>
        <errorDisplayField>PersonMobilePhone</errorDisplayField>
        <errorMessage>Mobile number must start with zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Start_Zero</fullName>
        <active>true</active>
        <description>Validates the Phone field to ensure it starts with zero.
MIrfan@Appirio, Amended on 160612 for Jira (PP-1499).</description>
        <errorConditionFormula>IF(
  OR(
     $Profile.Name = &quot;TMG System&quot;,
     $Profile.Name = &quot;System Administrator&quot;, 
     $Profile.Name = &quot;Zuora 360 Sync&quot;,
     $Profile.Name = &quot;System Administrator (Custom)&quot;
    ),
  false,
  AND(
    NOT(BEGINS( Phone,&quot;0&quot;)),
    OR(
       PersonMailingCountry=&apos;GB&apos;,
       PersonMailingCountry=&apos;UK&apos;)
    )
)</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>Phone number must start with zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Chaining_Duplicates</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(Duplicate_Of__r.Duplicate_Of__c)),
NOT($Profile.Name = &quot;Zuora 360 Sync&quot;)
)</errorConditionFormula>
        <errorMessage>The account you are trying to assign the duplicate already is marked as duplicate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Price_Plan_Zero</fullName>
        <active>false</active>
        <description>Only system or subs administrator can choose price plan 0</description>
        <errorConditionFormula>AND(
    OR(
        $RecordType.Name = &quot;Individual Subscriber&quot;,
        $RecordType.Name == &quot;Registrant&quot;
      ),
    $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
    $Profile.Name &lt;&gt; &quot;System Administrator (Custom)&quot;,
    $Profile.Name &lt;&gt; &quot;TMG System&quot;,
    $Profile.Name &lt;&gt; &quot;TMG Subs Administrator&quot;,
    $Profile.Name &lt;&gt; &quot;Zuora 360 Sync&quot;,
    ISPICKVAL( Price_Plan__c , &quot;Plan 0&quot;)
   )</errorConditionFormula>
        <errorDisplayField>Price_Plan__c</errorDisplayField>
        <errorMessage>Only System Administrator or Subs Administrator users can select Price Plan 0</errorMessage>
    </validationRules>
