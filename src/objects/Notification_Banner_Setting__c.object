<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>The settings that are used in NotificationBanner component</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Activate/Deactivate notification banner</description>
        <externalId>false</externalId>
        <inlineHelpText>Activate/Deactivate notification banner</inlineHelpText>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Fields__c</fullName>
        <description>API name of the field(s) that values are required to be displayed in notification message. Multiple fields must be separated by comma.</description>
        <externalId>false</externalId>
        <inlineHelpText>API name of the field(s) that values are required to be displayed in notification message. Multiple fields must be separated by comma.</inlineHelpText>
        <label>Fields</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Message__c</fullName>
        <description>The message that need to be displayed on the banner. This message can contain HTML and String formatting to display values from the fields specified in Field .</description>
        <externalId>false</externalId>
        <inlineHelpText>The message that need to be displayed on the banner. This message can contain HTML and String formatting to display values from the fields specified in Field .</inlineHelpText>
        <label>Message</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Object__c</fullName>
        <description>The API name of the object for which the notification is displayed</description>
        <externalId>false</externalId>
        <inlineHelpText>The API name of the object for which the notification is displayed</inlineHelpText>
        <label>Object</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Severity__c</fullName>
        <description>Use one of the key words ERROR, WARNING, INFO, SUCCESS</description>
        <externalId>false</externalId>
        <inlineHelpText>Use one of the key words ERROR, WARNING, INFO, SUCCESS</inlineHelpText>
        <label>Severity</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Notification Banner Settings</label>
    <visibility>Protected</visibility>
</CustomObject>
