    <fields>
        <fullName>Dynamic_Price__c</fullName>
        <description>Price is calculated in Pence based on Product Type, Subscription Type and respective TCode prices.</description>
        <externalId>false</externalId>
        <formula>IF(	ISPICKVAL(Product_Type__c, &quot;premium&quot;),
	IF(ISPICKVAL(Subscription_Type__c, &quot;52continuous&quot;),
		TCode_Product__r.Weekly_Price__c * 52 - 
		IF(ISPICKVAL(TCodeManagement__r.Type__c, &quot;Low Start&quot;),
		 VALUE(TEXT(TCodeManagement__r.Low_Start_Period__c)) * (TCode_Product__r.Weekly_Price__c - TCode_Product__r.Low_Start_Price__c),
		 0
		),
		IF(ISPICKVAL(Subscription_Type__c, &quot;26continuous&quot;),
			TCode_Product__r.Weekly_Price__c * 26 - 
			IF(ISPICKVAL(TCodeManagement__r.Type__c, &quot;Low Start&quot;),
		 		VALUE(TEXT(TCodeManagement__r.Low_Start_Period__c)) * (TCode_Product__r.Weekly_Price__c - TCode_Product__r.Low_Start_Price__c),
				 0
			),
			IF(ISPICKVAL(Subscription_Type__c, &quot;52weeks&quot;),
				TCode_Product__r.Weekly_Price__c * 52, 
				IF(ISPICKVAL(Subscription_Type__c, &quot;26weeks&quot;),
					TCode_Product__r.Weekly_Price__c * 26,
					IF(ISPICKVAL(Subscription_Type__c, &quot;13continuous&quot;),
						TCode_Product__r.Weekly_Price__c * 13,
						IF(ISPICKVAL(Subscription_Type__c, &quot;13weeks&quot;),
							TCode_Product__r.X13_Week_Price__c,
							0
						)
					)
				)
			)
		)
	),
	IF(ISPICKVAL( Product_Type__c, &quot;Webpack&quot;),
		IF(ISPICKVAL( Subscription_Type__c, &quot;yearly&quot;),
			TCode_Product__r.Web_Yearly_Price__c, 
			IF(ISPICKVAL(Subscription_Type__c,&quot;monthly&quot;),
				TCode_Product__r.Web_Monthly_Price__c,
				0
			)
		),				
		IF(ISPICKVAL(Product_Type__c, &quot;digital&quot;),
			IF(ISPICKVAL(Subscription_Type__c, &quot;yearly&quot;), 
				TCode_Product__r.Digital_Yearly_Price__c , 
				IF(ISPICKVAL(Subscription_Type__c,&quot;monthly&quot;), 
					TCode_Product__r.Digital_Monthly_Price__c ,
					0
				)
			),
			0
		)
	)
) * 100</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Price is calculated in Pence based on Product Type, Subscription Type and respective TCode prices.</inlineHelpText>
        <label>Dynamic Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
