    <webLinks>
        <fullName>Manage_Mid_Term_Discounts</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Manage Mid Term Discounts</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var subscrptionStatus=&apos;{!Zuora__Subscription__c.Zuora__Status__c}&apos;;
var subscriptionRatePlanTermType=&apos;{!Zuora__Subscription__c.Subscription_Rate_Plan_Term_Type__c}&apos;;
var subscriptionId=&apos;{!Zuora__Subscription__c.Id}&apos;;

if(subscrptionStatus==&apos;Active&apos; || subscrptionStatus==&apos;Pending Activation&apos;){

  if(subscriptionRatePlanTermType==&apos;Annual&apos;){
     alert(&apos;{!$Label.Mid_Term_Discounts_Annual_Subscription_Error}&apos;);
  }else{
      window.open(&apos;/apex/Zuora_ManageMidTermDiscounts?subId=&apos;+subscriptionId,&apos;_self&apos;);
  }
}else{
  alert(&apos;{!$Label.Mid_Term_Discount_Status_Error_Message}&apos;);
}</url>
    </webLinks>
