    <validationRules>
        <fullName>Banking_status_required</fullName>
        <active>true</active>
        <description>Cheque banking status required for a sub payment cheque</description>
        <errorConditionFormula>ISPICKVAL(  Cheque_Banking_Status__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Cheque_Banking_Status__c</errorDisplayField>
        <errorMessage>Cheque Banking Status is required for a Sub Payment cheque</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventChequeRefundOnSubscription</fullName>
        <active>true</active>
        <description>Validation rule to prevent cheque refunds being allocated to subscriptions with a status of &apos;Offered&apos;, &apos;Declined&apos; or &apos;Withdrawn&apos;.</description>
        <errorConditionFormula>IF( OR($Profile.Name = &quot;TMG System&quot;, $Profile.Name = &quot;TMG Subs Administrator&quot;), false,
AND(
OR(ISPICKVAL( Subscription__r.Subscription_Status__c , &apos;Offered&apos;),
ISPICKVAL( Subscription__r.Subscription_Status__c , &apos;Withdrawn&apos;),
ISPICKVAL( Subscription__r.Subscription_Status__c , &apos;Declined&apos;)
) ,
Or(
ISPICKVAL(Type__c, &apos;Refund&apos;) ,
ISPICKVAL(Type__c, &apos;Refund - awaiting approval&apos;)
)
)
)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>A cheque refund cannot be allocated to an Offered Withdrawn or Declined subscription</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refund_reason_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
       ISPICKVAL(Type__c , &quot;Refund&quot;),
       ISPICKVAL(Type__c, &quot;Refund - awaiting approval&quot;),
       ISPICKVAL( Type__c, &quot;Indemnity Cheque&quot;)
      ),
ISPICKVAL( Cheque_Refund_Reason__c , &quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>Cheque_Refund_Reason__c</errorDisplayField>
        <errorMessage>Refund reason is required for any cheque refund</errorMessage>
    </validationRules>
