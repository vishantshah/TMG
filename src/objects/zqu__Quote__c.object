    <webLinks>
        <fullName>Add_Default_Offer_Code</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Default Offer Code</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var confirmValue = confirm(&apos;{!$Label.Default_Offer_Code_Addition_Warning_Message}&apos;);
if (confirmValue == true) {
var url=&apos;/apex/ZuoraDefaultOfferCodeCheck?Id={!zqu__Quote__c.Id}&apos;;
window.open(url,&apos;_self&apos;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Add_Payment_Method</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Payment Method</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var newSubscriptionOfferCount=&apos;{!zqu__Quote__c.New_Subscription_Offer_Codes_Count__c}&apos;;
var quoteSubscriptionType=&apos;{!zqu__Quote__c.zqu__SubscriptionType__c}&apos;;

if(quoteSubscriptionType!=&apos;New Subscription&apos; || (quoteSubscriptionType==&apos;New Subscription&apos; &amp;&amp; newSubscriptionOfferCount &gt; 0)){
window.open(&apos;{!$Setup.Configuration__c.Visualforce_Base_Url__c}&apos; + &apos;Zuora_PaymentStep?id={!zqu__Quote__c.Id}&amp;action=add&apos;,&apos;_self&apos;);
}else{
  alert(&apos;{!$Label.Default_Offer_Code_or_Campaign_Offer_Required_Error_Message}&apos;);
}</url>
    </webLinks>
     <webLinks>
        <fullName>Submit_Finalised_Details</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit Finalised Details</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var newSubscriptionOfferCount=&apos;{!zqu__Quote__c.New_Subscription_Offer_Codes_Count__c}&apos;;
var quoteSubscriptionType=&apos;{!zqu__Quote__c.zqu__SubscriptionType__c}&apos;;

if(quoteSubscriptionType!=&apos;New Subscription&apos; || (quoteSubscriptionType==&apos;New Subscription&apos; &amp;&amp; newSubscriptionOfferCount&gt;0))
{

if({!zqu__Quote__c.arePaymentDetailsEntered__c}){

if(&apos;{!zqu__Quote__c.zqu__SubscriptionType__c}&apos;==&apos;Amend Subscription&apos; &amp;&amp; &apos;{!zqu__Quote__c.Mid_term_Discount_Reason__c}&apos;==&apos;&apos;){
  alert(&apos;{!$Label.Mid_term_Discount_Reason_Error_Message}&apos;);
}else{
window.open(&apos;{!$Setup.Configuration__c.Visualforce_Base_Url__c}&apos; + &apos;Zuora_sendToZBilling?quoteId={!zqu__Quote__c.Id}&amp;PayMethodId={!zqu__Quote__c.zqu__ElectronicPaymentMethodId__c}&amp;sfAccountId={!CASESAFEID(zqu__Quote__c.zqu__AccountId__c)}&apos;,&apos;_self&apos;);
}
}
else{
   window.open(&apos;{!$Setup.Configuration__c.Visualforce_Base_Url__c}&apos; + &apos;Zuora_PaymentStep?id={!zqu__Quote__c.Id}&amp;action=send&apos;,&apos;_self&apos;);
}
}else{
  alert(&apos;{!$Label.Default_Offer_Code_or_Campaign_Offer_Required_Error_Message}&apos;);
}</url>
