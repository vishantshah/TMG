    <fields>
        <fullName>if_11_Week_Renewal_Date_Hidden__c</fullName>
        <description>Interface field for New Logic Export. Calculates the date of the Saturday 11 weeks before previous subscription expiry.</description>
        <externalId>false</externalId>
        <formula>IF(
  Previous_Subscription__r.Subscription_Duration_Number__c == 13,
  Null, 
  Start_Date__c -((11*7)+2)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>R01 Extraction Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>if_2_Week_Renewal_Date_Hidden__c</fullName>
        <description>Interface field for New Logic Export. Calculates the date of the Saturday 2 weeks before previous subscription expiry.</description>
        <externalId>false</externalId>
        <formula>Start_Date__c - ((2*7)+2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>IF 2 Week Renewal Date Hidden</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>if_4_Week_DD_Increase_Extraction_Date__c</fullName>
        <description>Interface field for New Logic Export. Calculates the date of the Saturday 4 weeks before the first DD payment date of a new subscription to notify of DD price increase.</description>
        <externalId>false</externalId>
        <formula>IF(MOD(IF_Next_DD_Payment_Date__c - DATE(1900, 1, 7), 7) = 1, IF_Next_DD_Payment_Date__c - 30, 
IF(MOD(IF_Next_DD_Payment_Date__c - DATE(1900, 1, 7), 7) = 2, IF_Next_DD_Payment_Date__c - 31, 
IF(MOD(IF_Next_DD_Payment_Date__c - DATE(1900, 1, 7), 7) = 3, IF_Next_DD_Payment_Date__c - 32, 
IF(MOD(IF_Next_DD_Payment_Date__c - DATE(1900, 1, 7), 7) = 4, IF_Next_DD_Payment_Date__c - 33, 
IF(MOD(IF_Next_DD_Payment_Date__c - DATE(1900, 1, 7), 7) = 5, IF_Next_DD_Payment_Date__c - 34, 
IF(MOD(IF_Next_DD_Payment_Date__c - DATE(1900, 1, 7), 7) = 6, IF_Next_DD_Payment_Date__c - 28, 
IF_Next_DD_Payment_Date__c - 29))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>IF 4 Week DD Increase Extraction Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
