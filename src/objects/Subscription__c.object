    <fields>
        <fullName>Outstanding_Balance_Voucher_Adjustment__c</fullName>
        <defaultValue>0</defaultValue>
        <description>This field is used to apply Voucher Adjustments to the Outstanding Balance field which can not be added using Workflows as it a Formula field.</description>
        <externalId>false</externalId>
        <label>Outstanding Balance Voucher Adjustment</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Outstanding_Balance__c</fullName>
        <externalId>false</externalId>
        <formula>IF (Payment_Details_Received__c = true, (Total_DD_Payment__c - Voucher_Amount_Used__c) + Outstanding_Balance_Voucher_Adjustment__c, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Outstanding Balance to Customer</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Outstanding_Balance_to_from_Customer__c</fullName>
        <description>28/04/10 SB: Change as per Richard Harte request
Old: Astra_Migration_Balance__c + Liab_Calc__c -  Voucher_Amount_Used__c +  Revenue_Write_off_Amount__c</description>
        <externalId>false</externalId>
        <formula>IF(End_Date__c&gt; DATE(2009,01,01), Astra_Migration_Balance__c + (Liab_Calc__c - Voucher_Amount_Used__c) + Revenue_Write_off_Amount__c, null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If positive, outstanding balance is to customer. If negative, customer owes TMG.</inlineHelpText>
        <label>Outstanding Balance to/(from) Customer</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Paid_Until_Calculated__c</fullName>
        <description>This field is populated with paid until date calculated by workflow</description>
        <externalId>false</externalId>
        <inlineHelpText>This field is populated with paid until date calculated by workflow</inlineHelpText>
        <label>Paid Until Calculated</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
