    <validationRules>
        <fullName>Revenue_writeoff_reason</fullName>
        <active>true</active>
        <description>Revenue_writeoff_reason</description>
        <errorConditionFormula>AND(
      ISPICKVAL( Revenue_Write_off_Reason__c , &quot;&quot;),
       Revenue_Write_off_Amount__c &gt; 0
       )</errorConditionFormula>
        <errorDisplayField>Revenue_Write_off_Reason__c</errorDisplayField>
        <errorMessage>Revenue write-off reason is required if the write-off amount is above zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Changed_Sub_Status_Offered</fullName>
        <active>false</active>
        <description>Ensures that the Start Date can not be changed unless the Subscription Status is offered.</description>
        <errorConditionFormula>IF(OR($Profile.Name = &quot;TMG System&quot;,$Profile.Name = &quot;System Administrator&quot;),false, IF(AND(NOT(ISPICKVAL(Subscription_Status__c, &quot;Offered&quot;)), ISCHANGED(Start_Date__c)), IF(AND(ISPICKVAL(Subscription_Status__c, &quot;Pending&quot;), ISCHANGED(Start_Date__c), Start_Date__c  &lt; Subscription_Start_Calc__c), false, true), false))</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>The Start Date can not be changed unless the Subscription Status is Offered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Second_Monday_Future</fullName>
        <active>true</active>
        <description>Validates the Start Date field to ensure it is at least the second Monday in the future.</description>
        <errorConditionFormula>AND(
	$RecordType.Name != &apos;Schools&apos;,
	NOT(OR($Profile.Name = &quot;TMG System&quot;, 
            $Profile.Name = &quot;System Administrator&quot;,
            $Profile.Name = &quot;System Administrator (Custom)&quot;)),
	ISBLANK(Previous_Subscription__c),
				OR(ISCHANGED(Start_Date__c), ISNEW()), 
	OR( Start_Date__c &lt; Today() + VALUE(Product__r.LeadDays__c) ,
		MOD(Start_Date__c - DATE(1900, 1, 7), 7) &lt;&gt; 1
	)
)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>The Start Date must be at least the second Monday in the future, and after 10/01/2011 for a Gift Subscription.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Second_Monday_Future_Renewal</fullName>
        <active>true</active>
        <description>After 23 days renewal implementation it validates the Start Date field to ensure it is at least the second Monday in the future.</description>
        <errorConditionFormula>AND(
$RecordType.Name != &apos;Schools&apos;,
NOT(OR( $Profile.Name = &quot;TMG System&quot;, $Profile.Name = &quot;System Administrator&quot;, $Profile.Name = &quot;System Administrator (Custom)&quot;)), 
NOT(ISBLANK( Previous_Subscription__c)), 
OR(ISCHANGED(Start_Date__c), ISNEW()), 
OR(MOD(Start_Date__c - DATE(1900, 1, 7), 7) &lt;&gt; 1,Start_Date__c &lt; TODAY()+ VALUE(Product__r.LeadDays__c))
)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>For renewals the Start Date must be at least the second Monday in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Not_Offered_Read_Only_Fields</fullName>
        <active>true</active>
        <description>This Validation Rule ensures these fields are read-only when the Subscription Status is not Offered (fields not covered by profile read-only settings): Subscription Product, Subscription Duration, Payment Type, Discount and Price Plan.</description>
        <errorConditionFormula>IF($Profile.Name = &quot;System Administrator&quot;,false, 
IF( $Profile.Name = &quot;TMG System&quot;,false,
IF(ISPICKVAL(Subscription_Status__c, &quot;Offered&quot;), False, OR(ISCHANGED(Product__c), ISCHANGED(Subscription_Duration_Picklist__c), ISCHANGED(Discount__c)))
)
)</errorConditionFormula>
        <errorMessage>The Subscription Product, Subscription Duration, Payment Type and Discount fields are Read-Only when the Subscription Status is NOT Offered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Not_Offered_Required_Fields</fullName>
        <active>true</active>
        <description>This Validation Rule ensures these fields are required when the Subscription Status is not Withdrawn or Declined: Weekly Price, Payment Type, Delivery Type and Voucher Pack Duration.</description>
        <errorConditionFormula>IF( $Profile.Name = &quot;TMG System&quot;,false,
	IF(
		OR(
			ISPICKVAL(Subscription_Status__c, &quot;Offered&quot;),
			ISPICKVAL(Subscription_Status__c, &quot;Withdrawn&quot;), 
			ISPICKVAL(Subscription_Status__c, &quot;Declined&quot;)
			)
		, False, OR(ISNULL(Weekly_Price__c), 
					ISPICKVAL(Payment_Type__c, &quot;&quot;), 
					ISPICKVAL(Delivery_Type__c , &quot;&quot;), 
					ISNULL(Voucher_Pack_Duration__c)
					)
		)
)</errorConditionFormula>
        <errorMessage>The Weekly Price, Payment Type, Delivery Type and Voucher Pack Duration fields are Required when the Subscription Status is NOT Offered, Withdrawn or Declined.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subscription_Status_Lock_All_Fields</fullName>
        <active>false</active>
        <description>This Validation Rule locks all the fields if the Lock All Fields field is checked.</description>
        <errorConditionFormula>IF( OR($Profile.Name = &quot;TMG System&quot;, $Profile.Name = &quot;System Administrator&quot;, $Profile.Name = &quot;TMG Super User&quot;, $Profile.Name = &quot;TMG Subs Administrator&quot;),false,
AND(Lock_All_Fields__c = True, NOT(ISCHANGED(Lock_All_Fields__c)),
         NOT(ISCHANGED( Total_DD_Payment__c )),
         NOT(ISCHANGED( Value_of_Vouchers_in_Hand__c )),
         NOT(ISCHANGED( Revenue_Write_off_Amount__c )),
         NOT(ISCHANGED(  Revenue_Write_off_Reason__c  )),
         NOT(ISCHANGED( Asset_Calc__c )),
         NOT(ISCHANGED( Liab_Calc__c )),
         NOT(ISCHANGED( Deferred_Calc_Force_Update__c )),
         NOT(ISCHANGED(  Sum_of_Payments__c  )),
         NOT(ISCHANGED(  Astra_Migration_Balance__c  )),
         NOT(ISCHANGED(  Voucher_Adjustment_Reason__c ))
        )
)</errorConditionFormula>
        <errorMessage>All fields are locked once the Subscription Status is Withdrawn, Declined, Cancelled or Lapsed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subscription_Status_Payment_Restriction</fullName>
        <active>false</active>
        <description>This Validation Rule ensures the Subscription Status can not be anything other than Offered, Withdrawn or Declined unless the Payment Details Received checkbox has been checked.</description>
        <errorConditionFormula>IF( $Profile.Name = &quot;TMG System&quot;,false,
IF(OR(ISPICKVAL(Subscription_Status__c, &quot;Offered&quot;), ISPICKVAL(Subscription_Status__c, &quot;Declined&quot;), ISPICKVAL(Subscription_Status__c, &quot;Withdrawn&quot;)), False, IF(Payment_Details_Received__c = False, True, False))
)</errorConditionFormula>
        <errorDisplayField>Subscription_Status__c</errorDisplayField>
        <errorMessage>Payment details must be received in order for the Subscription Status to be Pending, Active, In Renewal, Pending Cancellation or Cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Voucher_Handling_Check</fullName>
        <active>false</active>
        <errorConditionFormula>IF( $Profile.Name = &quot;TMG System&quot;,false,
AND( 
  NOT( Retailer__r.Home_Delivery_Accepted__c),  
  ISPICKVAL(Delivery_Type__c ,&apos;HND&apos; )
)
)</errorConditionFormula>
        <errorDisplayField>Retailer__c</errorDisplayField>
        <errorMessage>Retailer must accept HND vouchers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Withdrawn_Selection</fullName>
        <active>true</active>
        <description>Only Admins can set a sub to Withdrawn</description>
        <errorConditionFormula>AND( 
$RecordType.Name = &quot;Default&quot;, 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;System Administrator (Custom)&quot;, 
$Profile.Name &lt;&gt; &quot;TMG Subs Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;TMG System&quot;, 
ISPICKVAL(Subscription_Status__c , &quot;Withdrawn&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Subscription_Status__c</errorDisplayField>
        <errorMessage>Only Admins can set a sub to Withdrawn</errorMessage>
    </validationRules>
