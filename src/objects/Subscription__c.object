        <type>Date</type>
    </fields>
    <fields>
        <fullName>Paid_Until__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Paid_Until_Calculated__c),(Start_Date__c-1) +(( (Total_of_All_Payments__c + Sum_of_Processed_Holiday_Return_Vouchers__c) / Total_Price__c )*7* Subscription_Duration_Number__c),
IF(OR(DD_Not_Completed_Payments_Sum__c = (Total_of_All_Payments__c + Sum_of_Processed_Holiday_Return_Vouchers__c), Weekly_Price__c = 0), Paid_Until_Calculated__c, (Paid_Until_Calculated__c - 1)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Paid Until</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
