        <fullName>Subscription_Start_Calc__c</fullName>
        <externalId>false</externalId>
        <formula>IF(LEN( Previous_Subscription__c)=0,
  IF(MOD(Today() - DATE(1900, 1, 7), 7) = 1, Today() + VALUE(Product__r.LeadDays__c) + 5, 
    IF(MOD(Today() - DATE(1900, 1, 7), 7) = 2, Today() + VALUE(Product__r.LeadDays__c) + 4, 
      IF(MOD(Today() - DATE(1900, 1, 7), 7) = 3, Today() + VALUE(Product__r.LeadDays__c) + 3, 
        IF(MOD(Today() - DATE(1900, 1, 7), 7) = 4, Today() + VALUE(Product__r.LeadDays__c) + 2, 
          IF(MOD(Today() - DATE(1900, 1, 7), 7) = 5, Today() + VALUE(Product__r.LeadDays__c) +1, 
            IF(MOD(Today() - DATE(1900, 1, 7), 7) = 6, Today() + VALUE(Product__r.LeadDays__c), 
        Today() + VALUE(Product__r.LeadDays__c) + 6)))))),
        
