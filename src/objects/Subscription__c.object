    <validationRules>
        <fullName>Canc_Date_After_Sub_End_Date</fullName>
        <active>true</active>
        <description>Checks to to make sure the Canx date isn&apos;t after the end date of the sub.</description>
        <errorConditionFormula>Cancellation_Date__c &gt; End_Date__c</errorConditionFormula>
        <errorDisplayField>Cancellation_Date__c</errorDisplayField>
        <errorMessage>Please check Cancellation date. It is after the subscription end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Canc_date_not_before_start_date</fullName>
        <active>true</active>
        <description>Checks the Canx date isn&apos;t before the Sub Start Date.</description>
        <errorConditionFormula>Start_Date__c &gt;Cancellation_Date__c</errorConditionFormula>
        <errorDisplayField>Cancellation_Date__c</errorDisplayField>
        <errorMessage>Please check the cancellation date. It is earlier than the start date of the sub.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Activate_if_Active_Digital</fullName>
        <active>true</active>
        <description>You cannot  activate print subscription if subscriber has any active digital subscription (Active, Pending, In Renewal).</description>
        <errorConditionFormula>AND(OR(ISNEW(), ISCHANGED(Subscription_Status__c)), OR(ISPICKVAL(Subscription_Status__c, &apos;Active&apos;), ISPICKVAL(Subscription_Status__c, &apos;In Renewal&apos;), ISPICKVAL(Subscription_Status__c, &apos;Pending&apos;)), Subscriber__r.Digital_Status__c = &apos;Active&apos;)</errorConditionFormula>
        <errorDisplayField>Subscription_Status__c</errorDisplayField>
        <errorMessage>You cannot set print subscription to Active, In Renewal or Pending if there is an existing Active digital subscription for the account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Is_Continuous</fullName>
        <active>true</active>
        <description>Jira:PP-1279
Update to the &quot;Is_Continuous&quot;.
Amended rule on 020512 to check only when Subscriber is on MPP not Payonomy (Mirfan@Appirio)
Amended rule on 090512 to skip TMG System Profile.</description>
        <errorConditionFormula>AND(Subscriber__r.IsUsingMPP__c, $Profile.Name &lt;&gt; &apos;TMG System&apos;,
OR(
AND( isContinuous__c=FALSE,ISPICKVAL(Payment_Type__c,&quot;Direct Debit&quot;)),
AND(
isContinuous__c=TRUE,
NOT( ISPICKVAL(Payment_Type__c,&quot;Direct Debit&quot;)),
ISBLANK(MPP_CC_Frequency__c)
),
AND(
isContinuous__c=FALSE,
NOT( ISPICKVAL(Payment_Type__c,&quot;Direct Debit&quot;)),
NOT(ISBLANK(MPP_CC_Frequency__c))
)
)
)</errorConditionFormula>
        <errorDisplayField>isContinuous__c</errorDisplayField>
        <errorMessage>IsContinuous is required for DD or when MPP CC Frequency is not blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DD_Subs_must_be_52_week_length</fullName>
        <active>false</active>
        <description>Ensure new Subscriptions are 52 week in length only.</description>
        <errorConditionFormula>IF(OR($Profile.Name = &quot;TMG System&quot;, NOT(Astra_Subscription_key__c = &quot;&quot;)), false, AND(NOT(ISPICKVAL(Subscription_Duration_Picklist__c, &quot;52&quot;)), ISPICKVAL( Payment_Type__c, &quot;Direct Debit&quot;)))</errorConditionFormula>
        <errorMessage>All Direct Debit Subscriptions must have a duration of 52 weeks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Gift_Value_for_Normal_Sub</fullName>
        <active>true</active>
        <description>A normal Sub can&apos;t have 13 weeks duration, gift donor, gift book or gift pack recipient.</description>
        <errorConditionFormula>IF( 
  OR(
    $Profile.Name = &quot;TMG System&quot;,
     RecordType.Name = &quot;Schools&quot;
  ),
  false, 
  AND( 
     Subscription_Duration_Number__c == 13, 
     NOT(ISBLANK(Previous_Subscription__c))
  ) 
)</errorConditionFormula>
        <errorMessage>A normal Sub can&apos;t have 13 weeks duration, gift donor, gift book or gift pack recipient.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Taken_Details_required</fullName>
        <active>true</active>
        <description>Validation for order taken details as part of acquisition feed</description>
        <errorConditionFormula>IF($RecordType.Name == &apos;Schools&apos;,
false,
AND( $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;TMG System&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator (Custom)&quot;,
DATEVALUE(IF(ISNULL(CreatedDate),
NOW(),
CreatedDate)
)&gt;= DATE(2009,8,22),
OR( Order_Taken_By_Company__c = &quot;&quot;,
Order_Taken_Date__c = NULL,
ISPICKVAL(Order_Taken_Method__c , &quot;&quot;),
TCode__c = NULL
)
)
)</errorConditionFormula>
        <errorMessage>Order Taken Details are required for every Subscription - please fill in details of the Date, Method, Wave and Company</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PaymentDateEqual1or15</fullName>
        <active>true</active>
        <description>PaymentDate Equals 1 or 15</description>
        <errorConditionFormula>AND(NOT(  ISNULL(PaymentDate__c) ), PaymentDate__c &lt;&gt; &apos;&apos;, PaymentDate__c &lt;&gt; &apos;1&apos;, PaymentDate__c &lt;&gt; &apos;15&apos;)</errorConditionFormula>
        <errorMessage>PaymentDate needs to be 1 or 15</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pending_Cancellation_Date_Required</fullName>
        <active>true</active>
