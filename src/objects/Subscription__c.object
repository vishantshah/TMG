        <fullName>if_Renewal_Letter_Version__c</fullName>
        <description>New Logic Interface to determine Letter Verison for Renewal Letters.</description>
        <externalId>false</externalId>
        <formula>IF(AND(TODAY()&gt;= if_11_Week_Renewal_Date_Hidden__c, TODAY()&lt;if_7_Week_Renewal_Date_Hidden__c), &quot;R01&quot;, IF(AND(TODAY()&gt;= if_7_Week_Renewal_Date_Hidden__c, TODAY()&lt;if_2_Week_Renewal_Date_Hidden__c),&quot;R02&quot;,IF(TODAY()&gt;=if_2_Week_Renewal_Date_Hidden__c,&quot;R03&quot;, &quot;&quot;)))</formula>
