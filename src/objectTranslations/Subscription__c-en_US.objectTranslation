    <validationRules>
        <errorMessage><!-- Please check Cancellation date. It is after the subscription end date. --></errorMessage>
        <name>Canc_Date_After_Sub_End_Date</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please check the cancellation date. It is earlier than the start date of the sub. --></errorMessage>
        <name>Canc_date_not_before_start_date</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- IsContinuous is required for DD or when MPP CC Frequency is not blank. --></errorMessage>
        <name>Check_for_Is_Continuous</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- All Direct Debit Subscriptions must have a duration of 52 weeks --></errorMessage>
        <name>DD_Subs_must_be_52_week_length</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please check that these values are valid for a Gift Sub:
-Weekly = 7.30, 7.00, 6.50 or 6.00
-Duration= 13 weeks
-Delivery Type= OTC
-Order Taken Method= Inbound
-Order Taken Method= 857T or 996T
-Select Gift Pack Recipient
-Select Gift Pack Book --></errorMessage>
        <name>Gift_Subs_RequiredValue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- A normal Sub can&apos;t have 13 weeks duration, gift donor, gift book or gift pack recipient. --></errorMessage>
        <name>No_Gift_Value_for_Normal_Sub</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Order Taken Details are required for every Subscription - please fill in details of the Date, Method, Wave and Company --></errorMessage>
        <name>Order_Taken_Details_required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- PaymentDate needs to be 1 or 15 --></errorMessage>
        <name>PaymentDateEqual1or15</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Enter a Cancellation Date if the Subscription Status is Pending Cancellation. --></errorMessage>
        <name>Pending_Cancellation_Date_Required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Enter a Cancellation Method if the Subscription Status is Pending Cancellation. --></errorMessage>
        <name>Pending_Cancellation_Method_Required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Enter a Cancellation Reason if the Subscription Status is Pending Cancellation. --></errorMessage>
        <name>Pending_Cancellation_Reason_Required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The subscription price is invalid --></errorMessage>
        <name>Price_Validation</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- A Retailer must not be specified as this Subscription is marked for Over The Counter delivery --></errorMessage>
        <name>Retailer_must_not_be_present_on_OTC_Subs</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- A Retailer must be specified as this Subscription is marked for Home Delivery --></errorMessage>
        <name>Retailer_required_for_HD_Subs</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Revenue write-off reason is required if the write-off amount is above zero. --></errorMessage>
        <name>Revenue_writeoff_reason</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Start Date can not be changed unless the Subscription Status is Offered. --></errorMessage>
        <name>Start_Date_Changed_Sub_Status_Offered</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Start Date must be at least the second Monday in the future, and after 10/01/2011 for a Gift Subscription. --></errorMessage>
        <name>Start_Date_Second_Monday_Future</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- For renewals the Start Date must be at least the fourth Monday in the future. --></errorMessage>
        <name>Start_Date_Second_Monday_Future_Renewal</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Subscription Product, Subscription Duration, Payment Type and Discount fields are Read-Only when the Subscription Status is NOT Offered. --></errorMessage>
        <name>Status_Not_Offered_Read_Only_Fields</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Weekly Price, Payment Type, Delivery Type and Voucher Pack Duration fields are Required when the Subscription Status is NOT Offered, Withdrawn or Declined. --></errorMessage>
        <name>Status_Not_Offered_Required_Fields</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- All fields are locked once the Subscription Status is Withdrawn, Declined, Cancelled or Lapsed. --></errorMessage>
        <name>Subscription_Status_Lock_All_Fields</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Payment details must be received in order for the Subscription Status to be Pending, Active, In Renewal, Pending Cancellation or Cancelled. --></errorMessage>
