<apex:page showHeader="false" controller="LiveChatPreForm">

    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta http-equiv="x-ua-compatible" content="IE=edge" />
        <title>Telegraph Live chat</title>
        <script type="text/javascript">
            (function() {
                function handlePageLoad() {
                    var endpointMatcher = new RegExp("[\\?\\&]endpoint=([^&#]*)");
                    document.getElementById('prechatForm').setAttribute('action',
                        decodeURIComponent(endpointMatcher.exec(document.location.search)[1]));
                }
                if (window.addEventListener) {
                    window.addEventListener('load', handlePageLoad, false);
                } else {
                    window.attachEvent('onload', handlePageLoad, false);
                }
            })();
        </script>
        <style>
            /* page; layout and defaults */
            html {
                font-size: 10px;
            }
            body {
                color: #353535;
                font-family: Arial,sans-serif;
                font-size: 1.4rem;
                line-height: 2rem;
                margin: 0;
                padding: 0;
                text-rendering: optimizeLegibility;
                -webkit-font-smoothing: antialiased;
                -moz-osx-font-smoothing: grayscale;
            }
            .wrapper {
                margin: 0 auto;
                max-width: 600px;
                width: 100%;
            }
            .wrapper--indented > * {
                margin-left: 20px;
                margin-right: 20px;
            }
        </style>
        <style>
            /* page formatting */
            .intro {
                border-bottom: 1px solid #ccc;
                sdisplay: none;
                min-height: 30px;
                padding-left: 30px;
                padding-bottom: 20px;
                position:relative;
            }
            .intro svg {
                width: 22px;
                height: auto;
                overflow: hidden;
                fill: #c38419;
                position: absolute;
                left: 0;
                bottom: 20px;
            }
        </style>
        <style>
            /* form */
            input,
            button,
            textarea,
            select,
            option {
                font-family: Arial,sans-serif;
                font-size: 1.4rem;
                line-height: 2rem;
            }
            button,
            input,
            select,
            textarea {
                border: 1px solid #ccc;
                box-sizing: border-box;
                padding: 4px;
                width: 100%;
            }
            button,
            input,
            select,
            textarea,
            option {
                color: #c38419;
            }
            select {
                -moz-appearance: none;
                -webkit-appearance: none;
                appearance: none;
                background: transparent;
                border-radius: 0;
                padding: 1.2rem 3.8rem 1.2rem 1.2rem;
                position: relative;
                z-index: 1;
            }
            select::-ms-expand {
                display: none;
            }
            input[type='text'],input[type='email'] {
                padding: 1.2rem 3.8rem 1.2rem 1.2rem;
            }
            .select-parent {
                color: #c38419;
                position: relative;
            }
            .select-parent svg {
                fill: currentcolor;
                height: 22px;
                position: absolute;
                right: 14px;
                top: 12px;
                width: 22px;
            }

            .form-group {
                margin-bottom: 12px;
            }
            .compulsory-message {
                display: block;
                margin-bottom: 12px;
                margin-top: -6px;
            }
            .control-label {
                display: block;
                margin-bottom: 3px;
            }

            /* buttons */
            .tmg-btn {
                border: 0;
                background: #c38419;
                border-radius: 0;
                color: #fff;
                cursor: pointer;
                font-size: 1.4rem;
                font-weight: normal;
                outline: 0;
                padding: 1.1rem 2.4rem;
                -webkit-transition: all 250ms linear;
                transition: all 250ms linear;
                text-align: center;
                text-decoration: none;
            }
            .tmg-btn.primary {
                background: #c38419;
                color: #fff;
            }
            .tmg-btn.primary:hover,
            .tmg-btn.primary:focus {
                background: #9e6710;
                color: #fff;
            }
            .tmg-btn[disabled],
            .tmg-btn[disabled]:hover,
            .tmg-btn[disabled]:focus {
                background-color: #afafaf;
                border-color: #afafaf;
                cursor: default;
                opacity: 1;
            }


            /* validation */
            .not-empty .control-label:after {
                content : "*";
            }
            .form-messaging-disabled .not-empty .control-label:after {
                content : "";
            }
            .form-messaging-disabled .compulsory-message {
                visibility: hidden;
            }
            .error {
                display: block;
                max-height: 0;
                overflow: hidden;
                transition: all 750ms linear;
            }
            .has-error .error {
                color: #a94442;
                max-height: 30px;
            }
        </style>
        <style>
            .liveAgentActiveChatHeader {
                /* telegraph branding */
                /* changing the height of this will have knock-on effects, see liveAgentActiveChatWrapper */
                border-bottom: 1px solid #dddcdc;
                height: 30px;
                margin: 0 20px;
                padding-top: 12px;
            }
            .liveAgentActiveChatHeader svg {
                height: auto;
                width: 130px;
            }
        </style>
        <style>
            /* navigation */
            .header-bar__close,
            .header-bar__save {
                display: none;
            }
        </style>
    </head>

