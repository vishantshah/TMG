            .header-bar {
                background-color: #353535;
                color: white;
            }
            .header-bar__content {
                display: flex;
                height: 50px;
                margin: 0 auto;
                max-width: 600px;
                padding: 0 5px 0 20px;
            }
            .header-bar__content > :last-child {
                margin-right: 16px;
            }
            .header-bar__logo {
                flex-basis: 40px;
            }
            .header-bar__title {
                font-size: 1.7rem;
                padding-left: 12px;
                padding-top: 17px;
                flex: 2;
            }
            .header-bar--graphic svg {
                max-height: 30px;
                max-width: 30px;
                height: auto;
                fill: white;
                width: auto;
            }
            .header-bar__logo.header-bar--graphic svg {
                margin-top: 11px;
            }
            .header-bar__button {
                background: #c38419;
                border: 0 none;
                border-radius: 0;
                color: #fff;
                cursor: pointer;
                font-size: 14px;
                line-height: 16px;
                margin: 8px 0 8px 8px;
                padding: 0 16px ;
                -webkit-transition: .25s;
                transition: .25s;
                width: auto;
            }
            .header-bar__button:hover,
            .header-bar__button:focus {
                background-color: #9e6710;
                outline: none;
            }
            .header-bar__button--icon {
                padding-left: 32px;
                position: relative;
            }
            .header-bar__button--icon svg {
                position: absolute;
                fill: currentcolor;
                height: 14px;
                left: 12px;
                top: 10px;
                width: 14px;
