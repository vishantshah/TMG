        <p>
            Please fill in this form to request a chat with
            one of our advisors.
        </p>
        <form method="post" id="prechatForm" class="form-horizontal">
            <!-- Used to set the visitor's name for the agent in the Console -->
            <input type="hidden" name="liveagent.prechat.name" id="prechat_field_name" />


            <div class="form-group not-empty">
                <label class="control-label" for="firstName">
                    First Name </label>
                <div>
                    <input type="text" name="liveagent.prechat:ContactFirstName" id='firstName' class="form-control"/>
                </div>
            </div>



            <div class="form-group not-empty">
                <label class="control-label" for="subject">
                    I have a question about </label>
                <div class="select-parent">
                    <select class="form-control" name="liveagent.prechat.buttons" id="subject">
                        <option>Please select</option>
                        <option value="{!serviceButtonId}">My current/existing subscription</option>
                        <option value="{!salesButtonId}">A new subscription</option>
                    </select>
                    <span class="error">Mandatory field</span>
                    <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                             width="1792px" height="1792px" viewBox="384 -512 1792 1792" enable-background="new 384 -512 1792 1792" xml:space="preserve">
                        <g>
                            <g>
                                <g>
                                    <g>
                                        <polygon points="1280,356 440,-92 384,-36 1280,860 2176,-36 2120,-92                "/>
                                    </g>
                                </g>
                            </g>
                        </g>
                    </svg>
                </div>
            </div>

            <small class="compulsory-message">* Compulsory field</small>
            <div class="form-group">
                <div>
                    <button class="tmg-btn primary" value="Request Chat" disabled="true" id="prechat_submit"  onclick="setName()">Request Chat</button>
                </div>
            </div>

            <!-- new Case setup -->
            <input type="hidden" name="liveagent.prechat:CaseRecordType" value="{!caseRT}" />
            <input type="hidden" name="liveagent.prechat:CaseStatus" value="New" />
            <input type="hidden" name="liveagent.prechat:CaseOrigin" value="Live Chat" />
            <input type="hidden" name="liveagent.prechat:CaseReason" value="Subscriptions" />
            <input type="hidden" name="liveagent.prechat:TypeOfIssue" value="Acquisition" />
            <input type="hidden" name="liveagent.prechat:CaseSubject" id="prechat_field_casesubject"/>

            <!-- map for Case -->
            <input type="hidden" name="liveagent.prechat.findorcreate.map:Case" value="Subject,CaseSubject;Status,CaseStatus;Origin,CaseOrigin;RecordTypeId,CaseRecordType;Reason,CaseReason;Type_Of_Issue__c,TypeOfIssue;Live_Chat_User_Name__c,ContactFirstName" />

            <!-- Create a Case -->
            <input type="hidden" name="liveagent.prechat.findorcreate.map.doCreate:Case" value="Subject,true;Status,true;Origin,true;RecordTypeId,true;Reason,true;Type_Of_Issue__c,true;Live_Chat_User_Name__c,true" />
            <input type="hidden" name="liveagent.prechat.findorcreate.showOnCreate:Case" value="true" />
            <input type="hidden" name="liveagent.prechat.findorcreate.saveToTranscript:Case" value="CaseId" />

            <!-- Set the visitor's name for the agent in the Console -->
            <script type="text/javascript">
                function setName() {
                    document.getElementById("prechat_field_name").value = document.getElementById("firstName").value;
                    document.getElementById("prechat_field_casesubject").value = document.getElementById("subject").options[document.getElementById("subject").selectedIndex].innerHTML;
                }
            </script>


        </form>

    </div>

    <script>
        (function(){

            function checkempty(){
                var nodesArray = Array.prototype.slice.call(document.querySelectorAll("div.not-empty .form-control"));
                var button = document.querySelector("#prechat_submit"),
                    valid = true;
                nodesArray.some(function(el){
                    if (!el.value || el.selectedIndex === 0){
                        valid = false; return true;
                    }
                    return false;
                });
                if (valid){
                    button.removeAttribute("disabled");
                } else {
                    button.setAttribute("disabled", "disabled");
                }
            }
            document.querySelector("body").classList.add("form-messaging-disabled");
            document.documentElement.addEventListener("keyup", checkempty, false);
            document.querySelector("#subject").addEventListener("change", checkempty, false);

        })();
    </script>
    <script>
        (function(){
            var closeButtons = document.querySelectorAll(".close-button"),
                fnWindowClose = function(){ window.close() };
            [].forEach.call(closeButtons, function(closeButton) {
                closeButton.addEventListener("click", fnWindowClose ,false);
            });
        })();
    </script>

</apex:page>